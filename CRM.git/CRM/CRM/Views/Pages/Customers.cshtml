                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                        <div class="input-group">
                            <span class="input-group-addon" id="sizing-addon2">Tel-1</span>
                            <input type="text" class="form-control" placeholder="Gsm-1" aria-describedby="sizing-addon2">
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                        <div class="input-group">
                            <span class="input-group-addon" id="sizing-addon2">Gsm-2</span>
                            <input type="text" class="form-control" placeholder="Gsm-2" aria-describedby="sizing-addon2">
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                        <div class="input-group">
                            <span class="input-group-addon" id="sizing-addon2">Fax</span>
                            <input type="text" class="form-control" placeholder="Fax" aria-describedby="sizing-addon2">
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                        <div class="input-group">
                            <span class="input-group-addon" id="sizing-addon2">Adres</span>
                            <input type="text" class="form-control" placeholder="Adres" aria-describedby="sizing-addon2">
                        </div>
                    </div>
                </div>
            </div>
